package com.huaao.ejwplatform.api.dto.deptinformation;

import java.util.ArrayList;
import java.util.List;

public class DeptInfoWrap {
	private String name;
	private DeptInfomationItem information = new DeptInfomationItem();
	private DeptPopulation population = new DeptPopulation();
	private DeptInstitution institution = new DeptInstitution();
	private DeptHot hot = new DeptHot();
	private DeptCourse course = new DeptCourse();
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public DeptInfomationItem getInformation() {
		return information;
	}
	public void setInformation(DeptInfomationItem information) {
		this.information = information;
	}
	public DeptPopulation getPopulation() {
		return population;
	}
	public void setPopulation(DeptPopulation population) {
		this.population = population;
	}
	public DeptInstitution getInstitution() {
		return institution;
	}
	public void setInstitution(DeptInstitution institution) {
		this.institution = institution;
	}
	public DeptHot getHot() {
		return hot;
	}
	public void setHot(DeptHot hot) {
		this.hot = hot;
	}
	public DeptCourse getCourse() {
		return course;
	}
	public void setCourse(DeptCourse course) {
		this.course = course;
	}
	

	
}
